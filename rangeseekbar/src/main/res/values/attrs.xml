<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RangeSeekBar">
        <attr name="thumbAndSeekBarColor" format="reference|color" />
        <attr name="thumbRadius" format="dimension" />
        <attr name="showPin" format="boolean" />
        <attr name="drawPinShadow" format="boolean" />
        <attr name="thumbToDraw" format="enum">
            <enum name="leftThumb" value="0" />
            <enum name="rightThumb" value="1" />
            <enum name="both" value="2" />
        </attr>
    </declare-styleable>
</resources>